FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    Walls
    {
       type            fixedValue;
       value              uniform 1e5;
    }

    bottom
    {
        type            zeroGradient;

    }

    top
    {
       type            fixedValue;
       value              uniform 1e5;
    }
}

